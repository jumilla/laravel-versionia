{
    "name": "jumilla/laravel-versionia",
    "type": "framework-extension",
    "description": "Version based database migration system for Laravel 5.",
    "license": "MIT",
    "keywords": ["Laravel", "Laravel 5", "databaes", "migration"],
    "homepage": "http://jumilla.me",
    "authors": [
        {
            "name": "Fumio Furukawa",
            "email": "fumio@jumilla.me"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "5.2.*",
        "illuminate/container": "5.2.*",
        "illuminate/contracts": "5.2.*",
        "illuminate/database": "5.2.*",
        "illuminate/support": "5.2.*",
        "jumilla/laravel-source-generator": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Jumilla\\Versionia\\Laravel\\": "sources/"
        }
    },
    "minimum-stability": "stable"
}
