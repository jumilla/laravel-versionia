{
    "name": "jumilla/laravel-versionia",
    "type": "framework-extension",
    "description": "Version based database migration system for Laravel 5.",
    "license": "MIT",
    "keywords": ["Laravel", "Laravel 5", "databaes", "migration"],
    "homepage": "http://jumilla.me",
    "authors": [
        {
            "name": "Fumio Furukawa",
            "email": "fumio@jumilla.me"
        }
    ],
    "require": {
		"php": "^7.0",
        "illuminate/console": "^5.5",
        "illuminate/container": "^5.5",
        "illuminate/contracts": "^5.5",
        "illuminate/database": "^5.5",
        "illuminate/support": "^5.5",
        "jumilla/laravel-source-generator": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Jumilla\\Versionia\\Laravel\\": "sources/"
        }
    },
	"extra": {
		"laravel": {
			"providers": [
				"Jumilla\\Versionia\\Laravel\\ServiceProvider"
			],
			"aliases": {
			}
		}
	},
    "minimum-stability": "stable"
}
